---
import '@/styles/base.scss';
import Head from './head.astro';
import CommonLayout from '@/components/Common/CommonLayout.astro';
type Props = {
  title: string;
  description: string;
};
const { title, description } = Astro.props;
---

<html lang="ja">
  <head>
    <Head title={title} description={description} />
  </head>
  <body>
    <CommonLayout>
      <slot />
    </CommonLayout>
  </body>
</html>
